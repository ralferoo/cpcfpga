    "xflow_program_optimized(array) " &
      "INITIALIZE           " &
        "(ISC_DATA_SHIFT    4096:?      wait TCK 1)" &
        "(ISC_ADDRESS_SHIFT 16:$addr=0  wait TCK 1)" &
